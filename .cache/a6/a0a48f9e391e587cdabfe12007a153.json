ject.keys(valores).length; i++) {\n                    values.push(valores[Object.keys(valores)[i]]);\n                    namevalue.push(Object.keys(valores)[i]);\n                }\n                const data = [{\n                    values: values,\n                    labels: namevalue,\n                    type: 'pie'\n                }];\n                const layout = {\n                    height: 300,\n                    width: 400\n                };\n                Plotly.plot(`${type}`, data, layout);\n            });\n    }\n}\n"},"lineCount":null}},"hash":"676f9111d1e0e60e43819689ebf4164d","cacheData":{"env":{}}}